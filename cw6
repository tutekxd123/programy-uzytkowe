2.
a=[],b=[],c=[],d=[],e=[]
i=1
while i<=100
	a(i)=1/i
	b(i)=1/i^2
	c(i)=1/sqrt(i)
	d(i)=(1+1/i)^i
	e(i)=(i^3-2*i^2+4)/2*i^3+i^2+10*i+14
i=i+1
end

3.
a)
result = 0
for i=1:20
result=(1/factorial(i))+result
end

b)
result = 0
for i=1:20
result=(2^i/factorial(i))+result
end

c)
result = 0
for i=1:40
result=((-1^i)/(factorial(2*i+1)))+result
end

d)
result=0
for i=1:1000
result=1/i^2+result
end

e)
result=0
for i=1:50
result=i^2/2^i
end
